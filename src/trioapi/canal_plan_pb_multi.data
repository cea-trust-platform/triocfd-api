# PARALLEL OK #

Dimension 2
Domaine dom

# BEGIN MESH #
Mailler dom
{
    pave bloc1
    {
        origine 0 0
        longueurs 100 0.2
        nombre_de_noeuds 10 3
    }
    {
        bord entree	 X = 0.            0. <= Y <= 0.2
        bord sortie	 X = 100	    0. <= Y <= 0.2
        bord bas	 Y = 0. 	    0. <= X <= 100
    } ,
    pave bloc2
    {
        origine 0 0.2
        longueurs 100 0.2
        nombre_de_noeuds 10 4
    }
    {
        bord entree	 X = 0	            0.2 <= Y <= 0.4
        bord sortie	 X = 100	    0.2 <= Y <= 0.4
    } ,
    pave bloc3
    {
        origine 0 0.4
        longueurs 100 0.6
        nombre_de_noeuds 10 7
    }
    {
        bord entree	 X = 0	            0.4 <= Y <= 1.0
        bord sortie	 X = 100	    0.4 <= Y <= 1.0
        bord haut	 Y = 1.0 	    0 <= X <= 100
    }
}

# END MESH #


# BEGIN PARTITION
Partition dom
{
    /* Choose Nb_parts so to have ~ 25000 cells per processor */
    Partition_tool metis { nb_parts 4 }
    Larg_joint 2
    zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

VEF dis

Schema_euler_implicite sch
Read sch
{
    tinit 0
    tmax 5
    dt_impr 1e-8
    facsec 1
    facsec_max 3
    seuil_statio 1e-10
    nb_pas_dt_max 3
    solveur Sets
    {
        criteres_convergence { alpha 1e-5 pression 1. vitesse 1e-5 temperature 1e8 k 1e-5 5 }
        iter_min 2
        solveur petsc cli { }
        seuil_convergence_implicite 1e30
    }
}

Pb_Multiphase pb


associate  pb dom
Associate  pb sch
Discretize pb dis

Lire pb
{
    Milieu_composite
    {
        liquide_eau Fluide_Incompressible
        {
            mu champ_uniforme      1 1
            rho champ_uniforme     1 1
            lambda Champ_Uniforme  1 1
            Cp Champ_Uniforme      1 1e3
            beta_th Champ_Uniforme 1 0
        }
    }
    correlations
    {
        loi_paroi adaptative { }
    }
    QDM_Multiphase
    {
        evanescence { homogene { alpha_res 1 alpha_res_min .5 } }
        solveur_pression petsc cli_quiet { -pc_type hypre -pc_hypre_type boomeramg -ksp_type fgmres }
        convection { amont }
        # diffusion  { turbulente $diffusion { sigma 1 } } #
        initial_conditions
        {
            vitesse  champ_fonc_xyz dom 2 3 0
            pression Champ_Fonc_xyz dom 1 2
        }
        conditions_limites
        {
            bas Paroi_frottante_loi {  /*  y_p_prod_k 1 fac_prod_k 0 y_p_prod_k_grand 1 fac_prod_k_grand 0 */ }
            haut symetrie
            entree frontiere_ouverte_vitesse_imposee_sortie Champ_front_Fonc_xyz 2 3 0
            sortie frontiere_ouverte_pression_imposee champ_front_uniforme 1 2
        }
    }
    Masse_Multiphase
    {
        initial_conditions { alpha Champ_Fonc_xyz dom 1 1 }
        convection { amont }
        conditions_limites
        {
            bas paroi
            haut symetrie
            entree	frontiere_ouverte a_ext Champ_Front_Uniforme 1 1
            sortie	frontiere_ouverte a_ext Champ_Front_Uniforme 1 1
        }
    }

    Energie_Multiphase
  {
    diffusion { }
    convection { amont }
    initial_conditions { temperature Champ_Uniforme 2 0 0 }
    boundary_conditions
    {
      wall paroi_adiabatique
      bottom frontiere_ouverte T_ext Champ_Front_Uniforme 2 0 0
      top frontiere_ouverte T_ext Champ_Front_Uniforme 2 0 0
      symetrie paroi_adiabatique
    }
    sources { flux_interfacial }
  }
    energie_cinetique_turbulente
    {
        diffusion { turbulente SGDH { sigma 0.67 } }
        convection { amont }
        initial_conditions { k champ_fonc_xyz dom 1 0.01 }
        boundary_conditions
        {
            haut symetrie
            entree	frontiere_ouverte k_ext Champ_Front_Uniforme 1 0.01
            sortie	frontiere_ouverte k_ext Champ_Front_Uniforme 1 0.01
        }
        sources
        {
            Production_energie_cin_turb { } ,
            Terme_dissipation_energie_cinetique_turbulente { beta_k 0.09 }
        }
    }
    Postraitement
    {
        sondes			{
            k	    	k      		periode 1e8 segment 10 1 2 3 4
            diss    	vitesse      		periode 1e8 position_like k
            vitesse    	vitesse_liquide_eau	periode 1e8 position_like k
            # dist_paroi	Distance_paroi		periode 1e8 position_like k #
            y_plus		y_plus			periode 1e8 segment 50 1 0 2 2
            pression	pression		periode 1e8 segment 1001	0 0.8	100 0.8
            pression_io	pression		periode 1e8 segment 2    0.75 0    0.75 100
            pression_i		pression		periode 1e8 segment 100     50 0	50 1
            pression_o		pression		periode 1e8 segment 100     100 0	100 1
            # tau_w	viscous_force		periode 1e8 segment 1001	0 0.0	100 0.0 #
        }
        format lata
		champs binaire dt_post 10.
			{
            k	    elem
            vitesse    elem
            vitesse_liquide_eau	elem
            y_plus	elem
            pression elem
        }

    }
}

Resoudre pb

Fin
